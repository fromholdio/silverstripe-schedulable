        "silverstripe/vendor-plugin": "^1.0",
        "silverstripe/framework": "^4.0"
    },
	"autoload": {
		"psr-4": {
			"Fromholdio\\Schedulable\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
