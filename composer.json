	"keywords": ["silverstripe", "embargo", "expiry", "schedule", "publishing", "publish date"],
	"homepage": "https://github.com/fromholdio/silverstripe-schedulable",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-schedulable/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
    "require": {
