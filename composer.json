{
	"name": "fromholdio/silverstripe-schedulable",
	"type": "silverstripe-vendormodule",
	"description": "This module adds embargo/expiry scheduling to your data objects and site tree.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "embargo", "expiry", "schedule", "publishing", "publish date"],
	"homepage": "https://github.com/fromholdio/silverstripe-schedulable",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-schedulable/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
    "require": {
        "silverstripe/cms": "^6.0",
        "fromholdio/silverstripe-cmsmessagefield": "^1.1.0"
    },
	"autoload": {
		"psr-4": {
			"Fromholdio\\Schedulable\\": "src/"
		}
	},
    "extra": {
        "branch-alias": {
            "dev-upgrade-v3": "3.0.x-dev"
        }
    },
	"prefer-stable": true,
	"minimum-stability": "dev"
}
